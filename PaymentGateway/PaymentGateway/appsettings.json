{
    "Logging": {
        "LogLevel": {
            "Default": "Debug",
            "Microsoft": "Warning",
            "Microsoft.Hosting.Lifetime": "Information"
        },
        "File": {
            "Path": "app.log",
            "Append": "True",
            "FileSizeLimitBytes": 5000, 
            "MaxRollingFiles": 5 
        },
        "AllowedHosts": "*"
    }
}
